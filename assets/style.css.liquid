/* TODO: Refactor into new Tile Component or Cart Sub-Component */
#cart,
#cart-nav {
	min-width: 60ch;
	--md-list-item-trailing-supporting-text-size: var(--md-sys-typescale-display-small-size);
	--md-list-item-trailing-supporting-text-font: var(--md-sys-typescale-display-small-font);
	--md-list-item-trailing-supporting-text-weight: var(--md-sys-typescale-display-small-weight);
	--md-list-item-label-text-font: var(--md-sys-typescale-display-medium-font);
	--md-list-item-label-text-size: var(--md-sys-typescale-display-medium-size);
	--md-list-item-label-text-weight: var(--md-sys-typescale-display-medium-weight);
	--md-list-item-label-text-line-height: var(--md-sys-typescale-display-large-line-height);
	--md-list-item-label-text-color: var(--md-sys-color-on-surface);

	& img {
		aspect-ratio: 1/1;
		object-fit: cover;
		max-width: 100px;
		border-radius: var(--border-radius-small);
	}

	& a {
		text-decoration: none;
		color: inherit;
	}

	& md-list {
		padding: var(--spacing-padding-medium);
	}

	& md-list-item {
		& div:nth-child(3) {
			font-size: var(--md-sys-typescale-display-small-size);
		}

		& div:last-child {
			min-width: 6ch;
		}
	}

	& md-outlined-text-field {
		max-width: 4ch;
	}

	& #cart-subtotal {
		--md-list-item-label-text-font: var(--md-sys-typescale-body-large-font);
		--md-list-item-label-text-size: var(--md-sys-typescale-body-large-size);
		--md-list-item-label-text-weight: var(--md-sys-typescale-body-large-weight);
		--md-list-item-label-text-line-height: var(--md-sys-typescale-body-large-line-height);
		border-radius: 0 0 var(--border-radius-small) var(--border-radius-small);

		& div:first-child {
			align-self: flex-end;
		}
	}

	& md-list-item:first-child {
		border-radius: var(--border-radius-small) var(--border-radius-small) 0 0;
	}

	& div:last-child {
		display: flex;
		justify-content: flex-end;
		gap: var(--spacing-gap-medium);
		flex-wrap: wrap;
	}
}

/* TODO: Refactor into new Tile Component or Cart Sub-Component */
#cart-nav {
	min-width: auto;
	--md-list-item-trailing-supporting-text-size: var(--md-sys-typescale-body-large-size);

	& img {
		max-width: 60px;
	}

	& md-list-item {
		& div:nth-child(3) {
			font-size: var(--md-sys-typescale-body-large-size);
		}

		& div:last-child {
			min-width: auto;
		}
	}

	& div:last-child {
		padding-right: var(--spacing-padding-medium);
	}
}
