@import url("https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200");
@import url("https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&display=swap");
@font-face {
	font-family: "ChlaraTypeface";
	src: url("https://assets.codepen.io/4999959/ChlaraTypeface-Regular.otf");
}

/* 
ROOTS
*/

/* COLLAGE SPA DEFAULTS */

:root {
	/* Collage Spa Prime Palette */
	--prime-green: #8fb765;
	--prime-yellow: #e6b63b;
	--prime-coral: #ed7079;
	--prime-teal: #17baab;
	--prime-blue: #2f3d5a;

	/* Collage Spa Palette */
	--cs-green: var(--prime-green);
	--cs-yellow: var(--prime-yellow);
	--cs-coral: var(--prime-coral);
	--cs-teal: var(--prime-teal);
	--cs-blue: var(--prime-blue);

	/* Collage Spa Fonts */
	--my-brand-font: "ChlaraTypeface";
	--my-headline-font: "Montserrat";
	--my-title-font: "Montserrat";
	--my-plain-font: "Montserrat";
}

/* COLOR */

:root {
	/* Material Design Color Presets */
	--md-sys-color-primary: #225bb7ff;
	--md-sys-color-on-primary: #ffffffff;
	--md-sys-color-primary-container: #d8e2ffff;
	--md-sys-color-on-primary-container: #001a43ff;
	--md-sys-color-primary-fixed: #d8e2ffff;
	--md-sys-color-primary-fixed-dim: #aec6ffff;
	--md-sys-color-on-primary-fixed: #001a43ff;
	--md-sys-color-on-primary-fixed-variant: #004397ff;
	--md-sys-color-secondary: #575e71ff;
	--md-sys-color-on-secondary: #ffffffff;
	--md-sys-color-secondary-container: #dbe2f9ff;
	--md-sys-color-on-secondary-container: #141b2cff;
	--md-sys-color-secondary-fixed: #dbe2f9ff;
	--md-sys-color-secondary-fixed-dim: #bfc6dcff;
	--md-sys-color-on-secondary-fixed: #141b2cff;
	--md-sys-color-on-secondary-fixed-variant: #3f4759ff;
	--md-sys-color-tertiary: #715573ff;
	--md-sys-color-on-tertiary: #ffffffff;
	--md-sys-color-tertiary-container: #fcd7fbff;
	--md-sys-color-on-tertiary-container: #2a132dff;
	--md-sys-color-tertiary-fixed: #fcd7fbff;
	--md-sys-color-tertiary-fixed-dim: #dfbbdeff;
	--md-sys-color-on-tertiary-fixed: #2a132dff;
	--md-sys-color-on-tertiary-fixed-variant: #583e5aff;
	--md-sys-color-error: #ba1a1aff;
	--md-sys-color-on-error: #ffffffff;
	--md-sys-color-error-container: #ffdad6ff;
	--md-sys-color-on-error-container: #410002ff;
	--md-sys-color-success: #4caf50ff;
	--md-sys-color-on-success: #ffffffff;
	--md-sys-color-success-container: #d7f3d9ff;
	--md-sys-color-on-success-container: #0a2a0aff;
	--md-sys-color-warning: #ff9800ff;
	--md-sys-color-on-warning: #ffffffff;
	--md-sys-color-warning-container: #ffe8d6ff;
	--md-sys-color-on-warning-container: #3f1a00ff;
	--md-sys-color-info: #2196f3ff;
	--md-sys-color-on-info: #ffffffff;
	--md-sys-color-info-container: #d6e8ffff;
	--md-sys-color-on-info-container: #001a43ff;
	--md-sys-color-background: #faf8fdff;
	--md-sys-color-on-background: #1b1b1fff;
	--md-sys-color-surface: #faf8fdff;
	--md-sys-color-on-surface: #1b1b1fff;
	--md-sys-color-surface-variant: #e1e2ecff;
	--md-sys-color-on-surface-variant: #44474fff;
	--md-sys-color-surface-dim: #dbd9ddff;
	--md-sys-color-surface-bright: #faf8fdff;
	--md-sys-color-surface-container-lowest: #ffffffff;
	--md-sys-color-surface-container-low: #f5f3f7ff;
	--md-sys-color-surface-container: #efedf1ff;
	--md-sys-color-surface-container-high: #e9e7ecff;
	--md-sys-color-surface-container-highest: #e3e2e6ff;
	--md-sys-color-surface-tint: #225bb7ff;
	--md-sys-color-outline: #757780ff;
	--md-sys-color-outline-variant: #c5c6d0ff;
	--md-sys-color-shadow: #000000ff;
	--md-sys-color-scrim: #000000ff;
	--md-sys-color-inverse-surface: #303034ff;
	--md-sys-color-inverse-on-surface: #f2f0f4ff;
	--md-sys-color-inverse-primary: #aec6ffff;
}

/* SPACING */

:root {
	/* Utopia Scale Minor to Major Second */
	--step--5: clamp(0.6938rem, 0.8555rem + -0.2087vw, 0.8138rem);
	--step--4: clamp(0.7806rem, 0.8986rem + -0.1522vw, 0.8681rem);
	--step--3: clamp(0.8781rem, 0.943rem + -0.0837vw, 0.9263rem);
	--step--2: clamp(0.9875rem, 0.9883rem + -0.0011vw, 0.9881rem);
	--step--1: clamp(1.0544rem, 1.0346rem + 0.0989vw, 1.1113rem);
	--step-0: clamp(1.125rem, 1.0815rem + 0.2174vw, 1.25rem);
	--step-1: clamp(1.2006rem, 1.1291rem + 0.3576vw, 1.4063rem);
	--step-2: clamp(1.2806rem, 1.1758rem + 0.5239vw, 1.5819rem);
	--step-3: clamp(1.3669rem, 1.2232rem + 0.7185vw, 1.78rem);
	--step-4: clamp(1.4581rem, 1.2688rem + 0.9467vw, 2.0025rem);
	--step-5: clamp(1.5556rem, 1.3132rem + 1.212vw, 2.2525rem);
	--step-6: clamp(1.66rem, 1.3559rem + 1.5207vw, 2.5344rem);
	--step-7: clamp(1.7713rem, 1.3958rem + 1.8772vw, 2.8506rem);
	--step-8: clamp(1.89rem, 1.4317rem + 2.2913vw, 3.2075rem);
	--step-9: clamp(2.0169rem, 1.4634rem + 2.7674vw, 3.6081rem);
	--step-10: clamp(2.1519rem, 1.4884rem + 3.3174vw, 4.0594rem);
}

/* TYPOGRAPHY */

:root {
	/* Default Font Variables */
	--md-ref-typeface-brand: var(--my-brand-font);
	--md-ref-typeface-plain: var(--my-plain-font);
	--md-ref-weight-bold: 700;
	--md-ref-weight-medium: 500;
	--md-ref-weight-regular: 400;

	--md-sys-typescale-body-font: var(--my-plain-font);
	--md-sys-typescale-display-font: var(--my-brand-font);
	--md-sys-typescale-headline-font: var(--my-headline-font);
	--my-sys-typescale-label-font: var(--my-plain-font);
	--md-sys-typescale-title-font: var(--my-title-font);

	/* Material Design Default Typescale */

	/* Display 
    Used for Page Titles and other High Priority Text
  */
	--md-sys-typescale-display-large-font: var(--md-sys-typescale-display-font);
	--md-sys-typescale-display-large-line-height: 1.2;
	--md-sys-typescale-display-large-size: var(--step-10);
	--md-sys-typescale-display-large-weight: 400;
	--md-sys-typescale-display-medium-font: var(--md-sys-typescale-display-font);
	--md-sys-typescale-display-medium-line-height: 1.2;
	--md-sys-typescale-display-medium-size: var(--step-6);
	--md-sys-typescale-display-medium-weight: 400;
	--md-sys-typescale-display-small-font: var(--md-sys-typescale-display-font);
	--md-sys-typescale-display-small-line-height: 1.2;
	--md-sys-typescale-display-small-size: var(--step-4);
	--md-sys-typescale-display-small-weight: 400;

	--md-sys-typescale-display-large: var(--md-sys-typescale-display-large-weight)
		var(--md-sys-typescale-display-large-size) / var(--md-sys-typescale-display-large-line-height)
		var(--md-sys-typescale-display-large-font);
	--md-sys-typescale-display-medium: var(--md-sys-typescale-display-medium-weight)
		var(--md-sys-typescale-display-medium-size) / var(--md-sys-typescale-display-medium-line-height)
		var(--md-sys-typescale-display-medium-font);
	--md-sys-typescale-display-small: var(--md-sys-typescale-display-small-weight)
		var(--md-sys-typescale-display-small-size) / var(--md-sys-typescale-display-small-line-height)
		var(--md-sys-typescale-display-small-font);

	/* Headline 
    Used for headlines that aren't the Page Title
  */
	--md-sys-typescale-headline-large-font: var(--md-sys-typescale-headline-font);
	--md-sys-typescale-headline-large-line-height: 1.2;
	--md-sys-typescale-headline-large-size: var(--step-4);
	--md-sys-typescale-headline-large-weight: 500;
	--md-sys-typescale-headline-medium-font: var(--md-sys-typescale-headline-font);
	--md-sys-typescale-headline-medium-line-height: 1.2;
	--md-sys-typescale-headline-medium-size: var(--step-3);
	--md-sys-typescale-headline-medium-weight: 500;
	--md-sys-typescale-headline-small-font: var(--md-sys-typescale-headline-font);
	--md-sys-typescale-headline-small-line-height: 1.2;
	--md-sys-typescale-headline-small-size: var(--step-2);
	--md-sys-typescale-headline-small-weight: 500;

	--md-sys-typescale-headline-large: var(--md-sys-typescale-headline-large-weight)
		var(--md-sys-typescale-headline-large-size) / var(--md-sys-typescale-headline-large-line-height)
		var(--md-sys-typescale-headline-large-font);
	--md-sys-typescale-headline-medium: var(--md-sys-typescale-headline-medium-weight)
		var(--md-sys-typescale-headline-medium-size) /
		var(--md-sys-typescale-headline-medium-line-height) var(--md-sys-typescale-headline-medium-font);
	--md-sys-typescale-headline-small: var(--md-sys-typescale-headline-small-weight)
		var(--md-sys-typescale-headline-small-size) / var(--md-sys-typescale-headline-small-line-height)
		var(--md-sys-typescale-headline-small-font);

	/* Title
    Generally for Subtitles and other Text that needs to stand out
  */
	--md-sys-typescale-title-large-font: var(--md-sys-typescale-title-font);
	--md-sys-typescale-title-large-line-height: 1.2;
	--md-sys-typescale-title-large-size: var(--step-5);
	--md-sys-typescale-title-large-weight: 500;
	--md-sys-typescale-title-medium-font: var(--md-sys-typescale-title-font);
	--md-sys-typescale-title-medium-line-height: 1.2;
	--md-sys-typescale-title-medium-size: var(--step-2);
	--md-sys-typescale-title-medium-weight: 500;
	--md-sys-typescale-title-small-font: var(--md-sys-typescale-title-font);
	--md-sys-typescale-title-small-line-height: 1.2;
	--md-sys-typescale-title-small-size: var(--step--1);
	--md-sys-typescale-title-small-weight: 500;

	--md-sys-typescale-title-large: var(--md-sys-typescale-title-large-weight)
		var(--md-sys-typescale-title-large-size) / var(--md-sys-typescale-title-large-line-height)
		var(--md-sys-typescale-title-large-font);
	--md-sys-typescale-title-medium: var(--md-sys-typescale-title-medium-weight)
		var(--md-sys-typescale-title-medium-size) / var(--md-sys-typescale-title-medium-line-height)
		var(--md-sys-typescale-title-medium-font);
	--md-sys-typescale-title-small: var(--md-sys-typescale-title-small-weight)
		var(--md-sys-typescale-title-small-size) / var(--md-sys-typescale-title-small-line-height)
		var(--md-sys-typescale-title-small-font);

	/* Body 
    The Main Text, as well as things like Lists, Text Fields, and Select Options
  */
	--md-sys-typescale-body-xlarge-font: var(--md-sys-typescale-body-font);
	--md-sys-typescale-body-xlarge-line-height: 1.5;
	--md-sys-typescale-body-xlarge-size: var(--step-1);
	--md-sys-typescale-body-xlarge-weight: 400;
	--md-sys-typescale-body-large-font: var(--md-sys-typescale-body-font);
	--md-sys-typescale-body-large-line-height: 1.5;
	--md-sys-typescale-body-large-size: var(--step--1);
	--md-sys-typescale-body-large-weight: 400;
	--md-sys-typescale-body-medium-font: var(--md-sys-typescale-body-font);
	--md-sys-typescale-body-medium-line-height: 1.5;
	--md-sys-typescale-body-medium-size: var(--step--2);
	--md-sys-typescale-body-medium-weight: 400;
	--md-sys-typescale-body-small-font: var(--md-sys-typescale-body-font);
	--md-sys-typescale-body-small-line-height: 1.5;
	--md-sys-typescale-body-small-size: var(--step--3);
	--md-sys-typescale-body-small-weight: 400;

	--md-sys-typescale-body-xlarge: var(--md-sys-typescale-body-xlarge-weight)
		var(--md-sys-typescale-body-xlarge-size) / var(--md-sys-typescale-body-xlarge-line-height)
		var(--md-sys-typescale-body-xlarge-font);
	--md-sys-typescale-body-large: var(--md-sys-typescale-body-large-weight)
		var(--md-sys-typescale-body-large-size) / var(--md-sys-typescale-body-large-line-height)
		var(--md-sys-typescale-body-large-font);
	--md-sys-typescale-body-medium: var(--md-sys-typescale-body-medium-weight)
		var(--md-sys-typescale-body-medium-size) / var(--md-sys-typescale-body-medium-line-height)
		var(--md-sys-typescale-body-medium-font);
	--md-sys-typescale-body-small: var(--md-sys-typescale-body-small-weight)
		var(--md-sys-typescale-body-small-size) / var(--md-sys-typescale-body-small-line-height)
		var(--md-sys-typescale-body-small-font);

	/* Label 
    Used for things like Form Labels, Buttons, Tags, and other small text
  */
	--md-sys-typescale-label-large-font: var(--my-sys-typescale-label-font);
	--md-sys-typescale-label-large-line-height: 1.5;
	--md-sys-typescale-label-large-size: var(--step-0);
	--md-sys-typescale-label-large-weight: 500;
	--md-sys-typescale-label-large-weight-prominent: 700;
	--md-sys-typescale-label-medium-font: var(--my-sys-typescale-label-font);
	--md-sys-typescale-label-medium-line-height: 1.5;
	--md-sys-typescale-label-medium-size: var(--step--1);
	--md-sys-typescale-label-medium-weight: 500;
	--md-sys-typescale-label-medium-weight-prominent: 700;
	--md-sys-typescale-label-small-font: var(--my-sys-typescale-label-font);
	--md-sys-typescale-label-small-line-height: 1.5;
	--md-sys-typescale-label-small-size: var(--step--2);
	--md-sys-typescale-label-small-weight: 500;
	--md-sys-typescale-label-xsmall-font: var(--my-sys-typescale-label-font);
	--md-sys-typescale-label-xsmall-line-height: 1.5;
	--md-sys-typescale-label-xsmall-size: var(--step--3);
	--md-sys-typescale-label-xsmall-weight: 500;


	--md-sys-typescale-label-large: var(--md-sys-typescale-label-large-weight)
		var(--md-sys-typescale-label-large-size) / var(--md-sys-typescale-label-large-line-height)
		var(--md-sys-typescale-label-large-font);
	--md-sys-typescale-label-medium: var(--md-sys-typescale-label-medium-weight)
		var(--md-sys-typescale-label-medium-size) / var(--md-sys-typescale-label-medium-line-height)
		var(--md-sys-typescale-label-medium-font);
	--md-sys-typescale-label-small: var(--md-sys-typescale-label-small-weight)
		var(--md-sys-typescale-label-small-size) / var(--md-sys-typescale-label-small-line-height)
		var(--md-sys-typescale-label-small-font);
	--md-sys-typescale-label-xsmall: var(--md-sys-typescale-label-xsmall-weight)
		var(--md-sys-typescale-label-xsmall-size) / var(--md-sys-typescale-label-xsmall-line-height)
		var(--md-sys-typescale-label-xsmall-font);
}

/* GLOBAL */

:root {
	/* Border Radius */
	--border-radius-xlarge: var(--step-9);
	--border-radius-large: var(--step-8);
	--border-radius-medium: var(--step-2);
	--border-radius-small: var(--step--4);
	--border-radius-xsmall: var(--step--5);
	--border-radius-none: 0;

	/* Spacing */
	/* --spacing-padding-large: var(--step-4); */
	/* --spacing-padding-medium: calc(var(--spacing-padding-large) / 2); */
	/* --spacing-padding-small: calc(var(--spacing-padding-medium) / 2); */
	/* --spacing-padding-xsmall: calc(var(--spacing-padding-small) / 2); */
	/* --spacing-padding-none: 0; */
	--spacing-padding-xlarge: var(--step-4);
	--spacing-padding-large: var(--step-2);
	--spacing-padding-medium: var(--step-0);
	--spacing-padding-small: var(--step--2);
	--spacing-padding-xsmall: var(--step--4);
	--spacing-padding-none: 0;

	--spacing-margin-xlarge: calc(var(--spacing-margin-large) * 2);
	--spacing-margin-large: clamp(1.4581em, 1.2688em + 0.9467vw, 2.0025em);
	--spacing-margin-medium: calc(var(--spacing-margin-large) / 2);
	--spacing-margin-small: calc(var(--spacing-margin-medium) / 2);
	--spacing-margin-xsmall: calc(var(--spacing-margin-small) / 2);
	--spacing-margin-none: 0;

	/* --spacing-gap-large: var(--step-4); */
	/* --spacing-gap-medium: calc(var(--spacing-gap-large) / 2); */
	/* --spacing-gap-small: calc(var(--spacing-gap-medium) / 2); */
	/* --spacing-gap-xsmall: calc(var(--spacing-gap-small) / 2); */
	/* --spacing-gap-none: 0; */

	--spacing-gap-xlarge: var(--step-4);
	--spacing-gap-large: var(--step-2);
	--spacing-gap-medium: var(--step-0);
	--spacing-gap-small: var(--step--2);
	--spacing-gap-xsmall: var(--step--5);
	--spacing-gap-xxsmall: calc(var(--step--5) / 2);
	--spacing-gap-none: 0;

	/* MATERIAL ELEMENTS DEFAULTS */

	/* Typography */
	--md-input-chip-label-text-size: var(--md-sys-typescale-label-small-size);

	/* Border Radius */
	--md-filled-button-container-shape: 999px;
	--md-filled-tonal-button-container-shape: var(--md-filled-button-container-shape);
	--md-outlined-button-container-shape: var(--md-filled-button-container-shape);
	--md-text-button-container-shape: var(--md-filled-button-container-shape);
	--md-menu-container-shape: var(--border-radius-xsmall);
	--md-fab-container-shape: var(--border-radius-small);
	--md-outlined-text-field-container-shape: var(--border-radius-xsmall);
	--md-outlined-select-text-field-container-shape: var(--border-radius-xsmall);

	/* Backgrounds */
	--md-list-container-color: none;
}

/* 
DEFAULT STYLES & RESETS
*/

body {
	/* display: grid;
    grid-auto-flow: dense;
	grid-template-columns: repeat(auto-fit, minmax(min(100%, 40ch), 1fr));
	grid-gap: var(--spacing-gap-medium); */
	background-color: var(--md-sys-color-surface-container);
	font: var(--md-sys-typescale-body-medium);
	color: var(--md-sys-color-on-background);
	transition: all 200ms ease;
	padding: var(--spacing-padding-medium);
	gap: var(--spacing-gap-medium);
	display: flex;
	flex-wrap: wrap;
	align-items: stretch;
	opacity: 0;
	margin: 0;
}

h1,
h2,
h3 {
	margin: 0;
	margin-block-start: var(--spacing-margin-medium);
	font: var(--md-sys-typescale-headline-large);
}

h2 {
	font: var(--md-sys-typescale-headline-medium);
}

h3 {
	font: var(--md-sys-typescale-headline-small);
}

p {
	max-width: 60ch;
	margin-block-start: .5em;
	margin-block-end: auto;
}

img {
	font-style: italic;
	background-repeat: no-repeat;
	background-size: cover;
	shape-margin: 1rem;
	height: auto;
	width: min(66%, 600px);
	vertical-align: middle;
	object-fit: cover;
	/* aspect-ratio: 1 / 1; */
}

/* Material Elements Defaults */

md-filled-button,
md-filled-tonal-button,
md-outlined-button,
md-text-button {
	font: var(--md-sys-typescale-label-medium);
}

/*
 BASIC HELPER CLASSES 
*/

/* TYPOGRAPHY & TYPESCALE */

.display--large {
	font: var(--md-sys-typescale-display-large);
	font-variant-ligatures: discretionary-ligatures;
	text-transform: uppercase;
	max-width: 40ch;
	-webkit-text-stroke-width: 0.01em;
	text-wrap: balance;
}
.display--medium {
	font: var(--md-sys-typescale-display-medium);
	max-width: 40ch;
	-webkit-text-stroke-width: 0.02em;
	text-wrap: balance;
}
.display--small {
	font: var(--md-sys-typescale-display-small);
	max-width: 40ch;
	-webkit-text-stroke-width: 0.04em;
	text-wrap: balance;
}

.headline--large {
	font: var(--md-sys-typescale-headline-large);
}
.headline--medium {
	font: var(--md-sys-typescale-headline-medium);
}
.headline--small {
	font: var(--md-sys-typescale-headline-small);
}

.title--large {
	font: var(--md-sys-typescale-title-large);
}
.title--medium {
	font: var(--md-sys-typescale-title-medium);
}
.title--small {
	font: var(--md-sys-typescale-title-small);
}

.body--xlarge {
	font: var(--md-sys-typescale-body-xlarge);
}
.body--large {
	font: var(--md-sys-typescale-body-large);
}
.body--medium {
	font: var(--md-sys-typescale-body-medium);
}
.body--small {
	font: var(--md-sys-typescale-body-small);
}

.label--large {
	font: var(--md-sys-typescale-label-large);
}
.label--medium {
	font: var(--md-sys-typescale-label-medium);
}
.label--small {
	font: var(--md-sys-typescale-label-small);
}

/* Tile Components - Shared Settings */
.tile {
    background: var(--md-sys-color-surface);
    color: var(--md-sys-color-on-background);
    padding: var(--spacing-padding-xlarge);
    border-radius: var(--border-radius-xlarge);
    overflow: clip;
  }

  .tile--large {
    width: min(100dvw, 1920px);
    /* grid-column: span 3; */
  }

  .tile--medium {
    width: min(50dvw, 1280px);
    flex: 1 1 auto;
    /* grid-column: span 2; */
  }

  .tile--small {
    width: min(30dvw, 640px);
	flex: 1 1 auto;
    /* grid-column: span 1; */
  }

  .tile--featured,
  .tile--hero {
    font: var(--md-sys-typescale-body-medium);
    padding: 0;

    & .tile__track {
      width: 100%;
    }
  }

  .tile--primary {
    background: var(--md-sys-color-primary-container);
    color: var(--md-sys-color-on-primary-container);
  }

  .tile--secondary {
    background: var(--md-sys-color-secondary-container);
    color: var(--md-sys-color-on-secondary-container);
    --md-sys-color-primary: var(--md-sys-color-secondary);
    --md-sys-color-on-primary: var(--md-sys-color-on-secondary);
    --md-sys-color-primary-container: var(--md-sys-color-secondary-container);
    --md-sys-color-on-primary-container: var(--md-sys-color-on-secondary-container);
    --md-sys-color-primary-fixed: var(--md-sys-color-secondary-fixed);
    --md-sys-color-primary-fixed-dim: var(--md-sys-color-secondary-fixed-dim);
    --md-sys-color-on-primary-fixed: var(--md-sys-color-on-secondary-fixed);
    --md-sys-color-on-primary-fixed-variant: var(--md-sys-color-on-secondary-fixed-variant);
  }

  .tile--tertiary {
    background: var(--md-sys-color-tertiary-container);
    color: var(--md-sys-color-on-tertiary-container);
    --md-sys-color-primary: var(--md-sys-color-tertiary);
    --md-sys-color-on-primary: var(--md-sys-color-on-tertiary);
    --md-sys-color-primary-container: var(--md-sys-color-tertiary-container);
    --md-sys-color-on-primary-container: var(--md-sys-color-on-tertiary-container);
    --md-sys-color-primary-fixed: var(--md-sys-color-tertiary-fixed);
    --md-sys-color-primary-fixed-dim: var(--md-sys-color-tertiary-fixed-dim);
    --md-sys-color-on-primary-fixed: var(--md-sys-color-on-tertiary-fixed);
    --md-sys-color-on-primary-fixed-variant: var(--md-sys-color-on-tertiary-fixed-variant);
  }

  .tile__track {
    position: sticky;
    top: var(--spacing-padding-xlarge);
    align-self: flex-start;
    margin: 0 auto;
    width: fit-content;
  }

  /* Required for making the Featured Tile responsive */
  .tile__container-query {
    container-type: inline-size;
  }

  .tile--images {
    background: none;
    padding: 0;

    .tile__track {
      display: flex;
      flex-wrap: wrap;
      gap: var(--spacing-gap-medium);
	  width: auto;
    }

    & img {
		width: min(30dvw, 640px);
		flex: 1 1 auto;

      border-radius: var(--border-radius-large);
    }
  }

  @media (max-width: 900px) {
    .tile--large,
    .tile--medium,
    .tile--small {
      width: 100%;
    }
  }

  .rich-text-block {
	max-width: 60ch;
  }



/* TODO: Move into CS specific group */
#cs-logo {
	min-height: 1rem;
	height: 100%;

	& .cs-logo__mask {
		fill: none;
	}

	& .cs-logo__green {
		fill: var(--cs-green);
	}

	& .cs-logo__yellow {
		fill: var(--cs-yellow);
	}

	& .cs-logo__coral {
		fill: var(--cs-coral);
	}

	& .cs-logo__teal {
		fill: var(--cs-teal);
	}
}


