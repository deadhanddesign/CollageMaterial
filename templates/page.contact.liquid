{% render 'component-header', title: 'Contact Us' %}

{% capture tile-content %}
  
  <img
    src="../assets/Surrealism_style_pattern.png"
    alt="Surrealism Style Pattern"
    id="theme-image"
    loading="eager"
    width="1200"
    height="1200"
  >
  
{% endcapture %}

{% render 'component-contact-form' %}

{% render 'component-tile', tile-content: tile-content, tile-type: 'images' %}

{% style %}
  .tile__track:has(#theme-image) {
    height: 100%;
  }
{% endstyle %}
